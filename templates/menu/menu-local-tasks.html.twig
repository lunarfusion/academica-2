{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  {{ include('academica:tabs', {
  variant: 'lifted',
  items: primary,
  size: 'sm',
  }, with_context: false) }}
{% endif %}
{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  {{ include('academica:tabs', {
  variant: 'bordered',
  items: secondary,
  }, with_context: false) }}
{% endif %}
